<?xml version="1.0" encoding="utf-8"?>
<androidx.drawerlayout.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/dra_layout">
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#ffffff"
        android:weightSum="667">
        <LinearLayout
            android:layout_weight="68"
            android:layout_width="match_parent"
            android:layout_height="0px">
            <include
                layout="@layout/toolbar"/>
        </LinearLayout>
        <LinearLayout
            android:layout_weight="67"
            android:layout_width="match_parent"
            android:layout_height="0px"></LinearLayout>
        <LinearLayout
            android:layout_weight="33"
            android:layout_width="match_parent"
            android:layout_height="0px">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="회원가입 완료!"
                android:textSize="28dp"
                android:textColor="#1d1d1d"
                android:gravity="center"/>
        </LinearLayout>

        <LinearLayout
            android:layout_weight="68"
            android:layout_width="match_parent"
            android:layout_height="0px"></LinearLayout>
        <LinearLayout
            android:layout_weight="90.3"
            android:layout_width="match_parent"
            android:layout_height="0px"
            android:orientation="horizontal"
            android:weightSum="375">
            <LinearLayout
                android:layout_weight="143"
                android:layout_width="0px"
                android:layout_height="match_parent"></LinearLayout>
            <LinearLayout
                android:layout_weight="89"
                android:layout_width="0px"
                android:layout_height="match_parent"
                android:background="@drawable/ic_z_logo"></LinearLayout>
        </LinearLayout>
        
        <LinearLayout
            android:layout_weight="46.6"
            android:layout_width="match_parent"
            android:layout_height="0px"></LinearLayout>
        <LinearLayout
            android:layout_weight="27"
            android:layout_width="match_parent"
            android:layout_height="0px"
            android:orientation="horizontal"
            android:weightSum="375">
            <LinearLayout
                android:layout_weight="16"
                android:layout_width="0px"
                android:layout_height="match_parent"></LinearLayout>
            <LinearLayout
                android:layout_weight="343"
                android:layout_width="0px"
                android:layout_height="match_parent">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:textColor="#1d1d1d"
                    android:textSize="22dp"
                    android:text="지금 바로 사이즈"
                    android:gravity="center"/>
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_weight="2.1"
            android:layout_width="match_parent"
            android:layout_height="0px"></LinearLayout>
        <LinearLayout
            android:layout_weight="29"
            android:layout_width="match_parent"
            android:layout_height="0px">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="ZEYO!"
                android:textSize="24dp"
                android:textColor="#1d1d1d"
                android:gravity="center"/>
        </LinearLayout>

        <LinearLayout
            android:layout_weight="138.1"
            android:layout_width="match_parent"
            android:layout_height="0px"></LinearLayout>
        <LinearLayout
            android:layout_weight="50"
            android:layout_width="match_parent"
            android:layout_height="0px"
            android:orientation="horizontal"
            android:weightSum="375">
            <LinearLayout
                android:layout_weight="16"
                android:layout_width="0px"
                android:layout_height="match_parent"></LinearLayout>
            <LinearLayout
                android:layout_weight="343"
                android:layout_width="0px"
                android:layout_height="match_parent">
                <Button
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/rectangle_signup"
                    android:textColor="#ffffff"
                    android:textSize="16dp"
                    android:text="시작!"
                    android:id="@+id/btn_start"/>
            </LinearLayout>
        </LinearLayout>

    </LinearLayout>
    <include
        layout="@layout/activity_drawer"/>
</androidx.drawerlayout.widget.DrawerLayout>